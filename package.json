{
  "name": "jch-ui",
  "version": "1.0.0",
  "description": "JCH",
  "scripts": {
    "preinstall": "node setup/node-version-check.js",
    "prepare-dev-config": "babel-node setup/prepare-config.js",
    "prestart": "npm-run-all --parallel remove-dist remove-config",
    "start": "npm run prepare-dev-config && cd setup && babel-node dev-server.js",
    "lint": "eslint webpack.config.* \"src/**/*.js\" \"src/**/*.jsx\" server --color",
    "test": "npm run lint && npm run specs",
    "specs": "mocha setup/test-setup.js \"test/**/*.spec.js\" --reporter spec",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "remove-config": "rimraf ./src/app/config/config.js",
    "prebuild": "npm run test && npm run clean-dist && npm run remove-config",
    "prepare-prod-config": "babel-node setup/prepare-config.js --env=production",
    "copy-images": "babel-node setup/copy-images",
    "build": "npm run copy-images && npm run prepare-prod-config && babel-node setup/build-html.js &&  babel-node setup/build.js",
    "postbuild": "babel-node setup/dist-server.js"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [],
  "author": "Aniket Kulkarni <aniket@triconinfotech.com> (http://github.com/anikettriconinfotech)",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.0.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "babel-register": "^6.11.6",
    "browser-sync": "^2.14.0",
    "colors": "^1.1.2",
    "connect-history-api-fallback": "^1.2.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.6.2",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "fs-extra": "^0.30.0",
    "isparta": "^4.0.0",
    "jsdom": "^9.4.1",
    "mocha": "^3.0.1",
    "npm-run-all": "^2.3.0",
    "open": "0.0.5",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.3.0",
    "react-hot-loader": "^3.0.0-beta.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1",
    "webpack-md5-hash": "0.0.5",
    "yargs": "^4.8.1"
  },
  "dependencies": {
    "formsy-material-ui": "^0.5.0",
    "formsy-react": "^0.18.1",
    "material-ui": "^0.15.3",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-router": "^2.6.1",
    "react-tap-event-plugin": "^1.0.0"
  }
}
